{
    "default": true,
    "extends": "@github/markdownlint-github",
    "MD004": {
        "style": "dash"
    },
    "MD007": {
        "indent": 2
    },
    "MD013": {
        "line_length": 120,
        "tables": false,
        "code_blocks": false
    },
    "MD022": {
        "lines_above": 1,
        "lines_below": 1
    },
    "MD024": {
        "siblings_only": true
    },
    "MD025": {
        "front_matter_title": ""
    },
    "MD029": {
        "style": "ordered"
    },
    "MD033": {
        "allowed_elements": [
            "br",
            "sub",
            "sup",
            "kbd",
            "details",
            "summary"
        ]
    },
    "MD040": {
        "allowed_languages": [],
        "language_only": false
    },
    "MD041": {
        "front_matter_title": ""
    },
    "MD046": {
        "style": "fenced"
    }
}